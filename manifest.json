{
   "background": {
      "scripts": [ "eventPage.js" ]
   },
   "content_scripts": [ {
      "css": [ "content.css" ],
      "js": [ "jquery-3.3.1.min.js", "content.js" ],
      "matches": [ "http://*/*", "https://*/*", "file:///*/*" ]
   } ],
   "description": "Check where a page have iframe or not",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
   },
   "manifest_version": 2,
   "name": "iFrame checker",
   "browser_action": {
      "default_icon": "icons/icon16.png",
      "default_popup": "popup.html",
      "default_title": "iFrameChecker"
   },
   "permissions": [ "tabs", "activeTab", "storage", "alarms" ],
   "version": "1.0"
}
